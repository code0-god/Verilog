#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x556584b6a810 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x556584b6a9a0 .scope module, "tb_counter" "tb_counter" 3 2;
 .timescale -9 -12;
v0x556584b7ba90_0 .var "clk", 0 0;
v0x556584b7bb60_0 .net "q", 3 0, v0x556584b7b860_0;  1 drivers
v0x556584b7bc30_0 .var "rst_n", 0 0;
S_0x556584b57260 .scope module, "dut" "counter" 3 6, 4 1 0, S_0x556584b6a9a0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /OUTPUT 4 "q";
P_0x556584b57440 .param/l "W" 0 4 1, +C4<00000000000000000000000000000100>;
v0x556584b57530_0 .net "clk", 0 0, v0x556584b7ba90_0;  1 drivers
v0x556584b7b860_0 .var "q", 3 0;
v0x556584b7b940_0 .net "rst_n", 0 0, v0x556584b7bc30_0;  1 drivers
E_0x556584b6b7a0/0 .event negedge, v0x556584b7b940_0;
E_0x556584b6b7a0/1 .event posedge, v0x556584b57530_0;
E_0x556584b6b7a0 .event/or E_0x556584b6b7a0/0, E_0x556584b6b7a0/1;
    .scope S_0x556584b57260;
T_0 ;
    %wait E_0x556584b6b7a0;
    %load/vec4 v0x556584b7b940_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x556584b7b860_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x556584b7b860_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x556584b7b860_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x556584b6a9a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556584b7ba90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556584b7bc30_0, 0, 1;
    %end;
    .thread T_1, $init;
    .scope S_0x556584b6a9a0;
T_2 ;
    %vpi_call/w 3 10 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call/w 3 11 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x556584b6a9a0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x556584b6a9a0;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x556584b7ba90_0;
    %inv;
    %store/vec4 v0x556584b7ba90_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x556584b6a9a0;
T_4 ;
    %delay 12000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556584b7bc30_0, 0, 1;
    %delay 200000, 0;
    %vpi_call/w 3 19 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "tb/tb_counter.v";
    "src/counter.v";
